<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='restclientapp'>
	<inherits name='com.google.gwt.user.User' />
	<inherits name="com.google.gwt.http.HTTP" />
	<inherits name="com.google.gwt.resources.Resources" />
	<inherits name="com.google.gwt.json.JSON" />
	<inherits name="com.google.gwt.xhr2.XMLHttpRequest2" />
	<inherits name="com.googlecode.gwt.math.Math"></inherits>
<!-- 	<inherits name="com.gwt.filesystem.LocalFileSystem"></inherits> -->

<!-- <inherits name="com.allen_sauer.gwt.log.gwt-log-DEBUG" /> -->
	<inherits name="com.allen_sauer.gwt.log.gwt-log-OFF" />
	<inherits name="com.google.code.gwt.database.Html5Database" />
<!-- <inherits name='com.google.gwt.user.theme.clean.Clean'/> -->
	<inherits name='com.google.gwt.user.theme.standard.Standard' />
<!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
<!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/> -->


	<entry-point class='com.restclient.client.RestClientApp' />
	<source path='client' />
	<source path='shared' />
	
	<!-- Compile for Chrome only -->
	<set-property name="user.agent" value="safari"/>
	
	<set-configuration-property name="UiBinder.useLazyWidgetBuilders" value="true"/>
	
	<extend-property name="log_level" values="DEBUG" />
	<set-property name="log_level" value="DEBUG" />
	<set-property name="log_DivLogger" value="DISABLED" />
	<set-property name="log_ConsoleLogger" value="ENABLED" />
</module>
